{
  "version": 3,
  "projects": [
    {
      "name": "frontend",
      "rootDirectory": "frontend",
      "buildCommand": "npm run build",
      "outputDirectory": ".next"
    },
    {
      "name": "backend",
      "rootDirectory": "backend",
      "buildCommand": "npm install",
      "routes": [
        { "src": "/(.*)", "dest": "/index.js" }
      ]
    }
  ]
}
